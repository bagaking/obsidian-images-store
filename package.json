{
  "name": "obsidian-images-store",
  "version": "0.1.0",
  "description": "This is a plugin for Obsidian (https://obsidian.md) that downloads images from external links and save in local vault, supports name pattern.",
  "main": "build/main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "rollup --config rollup.config.js --environment BUILD:dev",
    "dogfood": "npm run build && cp ./build/main.js /Users/bytedance/Sync/bk.barn/.obsidian/plugins/obsidian-images-store/main.js",
    "version": "json -I -f manifest.json -e \"this.version='$npm_package_version'\" && json -I -f versions.json -e \"this['$npm_package_version']='$(cat manifest.json | json minAppVersion)'\" && git add ."
  },
  "keywords": [],
