    "@types/node": "^14.18.20",
    "@types/safe-regex": "^1.1.2",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "json": "^11.0.0",
    "obsidian": "^0.12.17",
    "prettier": "^2.6.2",
    "rollup": "^2.75.5",
    "rollup-plugin-copy": "^3.4.0",
